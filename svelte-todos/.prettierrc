{
	"printWidth": 100,
	"tabWidth": 4,
	"useTabs": true,
	"semi": true,
	"singleQuote": true,
	"trailingComma": "all",
	"bracketSpacing": true,
	"arrowParens": "avoid",
	"proseWrap": "never",
	"endOfLine": "auto",
	"jsxBracketSameLine": false,
	"jsxSingleQuote": false,
	"vueIndentScriptAndStyle": true,
	"requirePragma": false,
	"insertPragma": false,
	"htmlWhitespaceSensitivity": "ignore",
	"plugins": ["esbenp.prettier-vscode"],
	"pluginSearchDirs": ["."],
	"overrides": [{ "files": "*.svelte", "options": { "parser": "svelte" } }]
}

